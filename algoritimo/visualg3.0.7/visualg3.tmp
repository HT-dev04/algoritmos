algoritmo "semnome"
var
   l1, l2, l3: real
   eq, esc: logico
inicio

         escreva("digite um numero: ")
         leia(l1)
         escreva("digite mais um numero: ")
         leia(l2)
         escreva("digite o ultimo numero: ")
         leia (l3)
         eq<-(l1=l2) e (l2=l3)
         esc<-( l1<>l2)e (l2<> l3)e (l1<>l3)
         escreval("o tringulo é equilatero? ", eq)
          escreval("o triangulo é escaleno? ", esc)
          
   
   
   

fimalgoritmo