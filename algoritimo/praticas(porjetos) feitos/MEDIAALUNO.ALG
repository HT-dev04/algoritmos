Algoritmo "mediaaluno"

Var
   n1,n2,n3,media:real



Inicio
   escreval("digite a primeira nota do aluno: ")
   leia(n1)
   escreval("digite a segunda nota do aluno: ")
   leia(n2)
   escreval("digite a terceira nota do aluno: ")
   leia(n3)
   media<-(n1+n2+n3)/3

   escreval("a media de nota do aluno sera: ",media)




Fimalgoritmo