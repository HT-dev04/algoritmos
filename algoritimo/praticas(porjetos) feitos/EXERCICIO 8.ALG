Algoritmo "exercicio 8"

Var
         peso,padrao,variacao:real


Inicio
           escreva("digite o peso: ")
           leia(peso)
           padrao<- 50
           variacao<- ((peso-padrao)/padrao)*100
           escreva(" a variaçao do peso em porcentagem é: ",variacao)


           

Fimalgoritmo